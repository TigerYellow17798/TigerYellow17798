local function DGWGW_fake_script() -- Frame.SoundService 
	local script = Instance.new('LocalScript', Frame)

	local player = Instance.new("Sound")
	player.Name = "RiskoPlayer"
	player.Parent = game.Workspace
end
coroutine.wrap(DGWGW_fake_script)()
