-- Scripts:

local function DOQCL_fake_script() -- Frame.Dragify 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0.50
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(DOQCL_fake_script)()
local function SFEA_fake_script() -- Exit.LocalScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(SFEA_fake_script)()
local function GILOML_fake_script() -- hide_unhide.LocalScript 
	local script = Instance.new('LocalScript', hide_unhide)

	local turn = true
	
	script.Parent.MouseButton1Down:Connect(function()
		if turn == true then
			turn = false
			script.Parent.Parent.Frame.Visible = true
			script.Parent.TextColor3 = Color3.fromRGB(255, 0, 4)
		else
			turn = true
			script.Parent.Parent.Frame.Visible = false
			script.Parent.TextColor3 = Color3.fromRGB(0, 170, 0)
		end
	end)
end
coroutine.wrap(GILOML_fake_script)()
local function OWRIKUC_fake_script() -- Diamonds.LocalScript 
	local script = Instance.new('LocalScript', Diamonds)

	script.Parent.MouseButton1Click:Connect(function()
		local bloky = game.Workspace.Blocks
	
		--Diamanty
		for i, v in pairs(bloky:GetChildren()) do
			local diamonds = Instance.new("Highlight")
			diamonds.FillColor = Color3.fromRGB(0, 85, 255)
			diamonds.Parent = v:FindFirstChild("DiamondOre")
		end
	end)
end
coroutine.wrap(OWRIKUC_fake_script)()
